def count_sort(arr):
    max_val = max(arr)
    count = [0] * (max_val)
    for num in arr:
        count[num] += 1
    sorted_arr = []
    for i in range(len(count)):
        for j in range(count[i]):
            sorted_arr.append(i)
    return sorted_arr

print(count_sort([4, 2, 2, 8, 3, 3, 1]))  # Expected [1, 2, 2, 3, 3, 4, 8]
